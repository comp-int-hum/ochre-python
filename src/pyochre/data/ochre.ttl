ochre:Coordinate
    owl:equivalentClass wde:Q3250736 ;
    sh:property [
        sh:path ochre:x ;
        sh:datatype xsd:integer ;
        sh:minInclusive 0 ;
    ] ;
    sh:property [
        sh:path ochre:y ;
        sh:datatype xsd:integer ;
        sh:minInclusive 0 ;
    ] .

ochre:BoundingBox
    a owl:Class ;
    a sh:NodeShape ;
    sh:property [
        sh:path ochre:topLeft ;
        sh:class ochre:Coordinate ;
    ] ;
    sh:property [
        sh:path ochre:bottomRight ;
        sh:class ochre:Coordinate ;
    ] .

